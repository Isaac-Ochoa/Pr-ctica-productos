<%- include("templates/encabezado") %>
    <h1>Lista de productos</h1>
 <div class="contenedor">
    <table>
        <thead>
    <tr>
        <th>id</th>
        <th>Nombre</th>
        <th>Descripcion</th>
        <th>Precio</th>
        <th>Editar / Borrar</th>
    </tr>
</thead>

 <tbody>
    <%  productosCorrectos.forEach((producto)=>{ %>
    <tr>
        <td><%= producto.id_prod %></td>
        <td><%= producto.nombre %></td>
        <td><%= producto.descripcion %></td>
        <td><%= producto.precio %></td>
        <td>
            <a href="/editarProducto/<%= producto.id_prod %>">Editar</a> / 
            <a href="/borrarProducto/<%= producto.id_prod %>">Borrar</a>
        </td>
    </tr>
 <% }); %>   
  </tbody>
</table>
</div>
    
    
<%- include("templates/pie") %>